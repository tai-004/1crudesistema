{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "img/icons/icon16.png",
      "default_title": "__MSG_ExtName__"
   },
   "default_locale": "en",
   "description": "__MSG_ExtShortDesc__",
   "differential_fingerprint": "",
   "externally_connectable": {
      "ids": [ "*" ],
      "matches": [ "*://*.avastbrowser.com/*", "*://*.avgbrowser.com/*", "*://*.ccleanerbrowser.com/*", "*://*.avast.com/*" ]
   },
   "icons": {
      "128": "img/icons/icon128.png",
      "16": "img/icons/icon16.png",
      "48": "img/icons/icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_ExtName__",
   "permissions": [ "management", "tabs", "\u003Call_urls>", "storage", "notifications", "unlimitedStorage", "webNavigation", "alarms" ],
   "version": "1.7.1.292",
   "web_accessible_resources": [ "popup.*" ]
}
